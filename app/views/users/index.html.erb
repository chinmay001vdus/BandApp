<div class="container">
<div class="container">
<h1>Userss</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Aboutus</th>
      <th>Schedule</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><strong><%= link_to user.firstname, user_path(user) %></strong></td>
        <td><%= user.lastname %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
<% if user_signed_in? %>
<%= link_to 'Back', user_path(@user), class: "btn btn-default" %>
<% else %>
<%= link_to 'Back', new_user_session_path, class: "btn btn-default" %>
<% end %>
</div>